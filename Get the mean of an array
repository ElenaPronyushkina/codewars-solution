function getAverage(marks){
  return Math.floor(marks.reduce((a, b) => a + b) / marks.length)
}


function getAverage(marks){
  let avg = 0;
  for (let i = 0; i < marks.length; i ++){
    avg += marks[i];
  }
  return Math.floor(avg / marks.length);
}
