//https://www.codewars.com/kata/5aa3af22ba1bb5209f000037/solutions/javascript

function solve(eq){
  return eq.split(/([-+*/])/).reverse().join('');
}


function solve(eq){
  return eq.replace(/\+/g, " + ")
           .replace(/\-/g, " - ")
           .replace(/\*/g, " * ")
           .replace(/\//g, " / ").split(" ").reverse().join('')
}
